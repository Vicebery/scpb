<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.scpb.dao.CoreEnterpriseDao">

	<select id="selectLimitById" resultType="java.lang.String">
		select `limit` from coreEnterprise where id = #{id}
	</select>

	<select id="selectSupplierById" resultType="Enterprise">
		select enterprise.id,account,pwd,bank,name,UCC,LPC,type from enterprise,coreEnterprise
		where coreEnterprise.id = #{id} and enterprise.id = coreEnterprise.mySupplier
	</select>

	<update id="updateLimitById">
		update coreEnterprise set `limit` = #{limit} where id = #{id}
	</update>

	<insert id="insertSupplier">
		insert into coreEnterprise(id,mySupplier) values (#{id},#{mySupplier})
	</insert>

	<delete id="deleteSupplier">
		delete from coreEnterprise where id = #{id} and mySupplier = #{mySupplier}
	</delete>
</mapper>